#include<stdio.h>
int main()
{
	char s[20];
	int i,vowels=0;
	printf("enter a line to find no of vowels:\n");
	scanf("%s",&s);
	for(i=0;s[i]!='\0';++i)
	{
		if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U')
		{
			++vowels;
		}
		
	}
		printf("%d",vowels);

	return 0;
}
